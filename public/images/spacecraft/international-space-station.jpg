<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="issGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1"/>
      <stop offset="50%" style="stop-color:#f0f0f0;stop-opacity:1"/>
      <stop offset="100%" style="stop-color:#e0e0e0;stop-opacity:1"/>
    </linearGradient>
    <radialGradient id="solarGradient" cx="50%" cy="50%" r="60%">
      <stop offset="0%" style="stop-color:#ffff00;stop-opacity:1"/>
      <stop offset="100%" style="stop-color:#ff8800;stop-opacity:1"/>
    </radialGradient>
  </defs>

  <!-- Background - Space -->
  <rect width="400" height="300" fill="#0a0a1a"/>

  <!-- ISS Main Structure -->
  <rect x="150" y="180" width="100" height="40" rx="5" fill="url(#issGradient)" stroke="#cccccc" stroke-width="2"/>
  <rect x="150" y="180" width="100" height="40" rx="5" fill="none" stroke="#aaaaaa" stroke-width="1" opacity="0.5"/>

  <!-- Modules -->
  <rect x="120" y="190" width="30" height="20" rx="3" fill="#e8e8e8" stroke="#cccccc" stroke-width="1"/>
  <rect x="250" y="190" width="30" height="20" rx="3" fill="#e8e8e8" stroke="#cccccc" stroke-width="1"/>
  <rect x="130" y="165" width="20" height="15" rx="2" fill="#f0f0f0" stroke="#cccccc" stroke-width="1"/>
  <rect x="250" y="165" width="20" height="15" rx="2" fill="#f0f0f0" stroke="#cccccc" stroke-width="1"/>

  <!-- Truss structure -->
  <rect x="100" y="200" width="200" height="8" rx="2" fill="#cccccc" stroke="#aaaaaa" stroke-width="1"/>
  <rect x="100" y="200" width="200" height="8" rx="2" fill="none" stroke="#888888" stroke-width="1" opacity="0.5"/>

  <!-- Solar panels -->
  <rect x="50" y="170" width="50" height="60" rx="3" fill="url(#solarGradient)" stroke="#ffaa00" stroke-width="1"/>
  <rect x="300" y="170" width="50" height="60" rx="3" fill="url(#solarGradient)" stroke="#ffaa00" stroke-width="1"/>
  <rect x="50" y="170" width="50" height="60" rx="3" fill="none" stroke="#ff8800" stroke-width="1" opacity="0.3"/>
  <rect x="300" y="170" width="50" height="60" rx="3" fill="none" stroke="#ff8800" stroke-width="1" opacity="0.3"/>

  <!-- Robotic arm -->
  <line x1="200" y1="220" x2="230" y2="240" stroke="#cccccc" stroke-width="4"/>
  <line x1="230" y1="240" x2="250" y2="235" stroke="#cccccc" stroke-width="4"/>
  <circle cx="200" cy="220" r="3" fill="#aaaaaa"/>
  <circle cx="230" cy="240" r="3" fill="#aaaaaa"/>
  <circle cx="250" cy="235" r="3" fill="#aaaaaa"/>

  <!-- Windows -->
  <rect x="160" y="185" width="8" height="6" rx="1" fill="#000033" opacity="0.8"/>
  <rect x="175" y="185" width="8" height="6" rx="1" fill="#000033" opacity="0.8"/>
  <rect x="190" y="185" width="8" height="6" rx="1" fill="#000033" opacity="0.8"/>
  <rect x="205" y="185" width="8" height="6" rx="1" fill="#000033" opacity="0.8"/>
  <rect x="220" y="185" width="8" height="6" rx="1" fill="#000033" opacity="0.8"/>
  <rect x="235" y="185" width="8" height="6" rx="1" fill="#000033" opacity="0.8"/>

  <!-- International flags -->
  <rect x="120" y="220" width="8" height="6" fill="#ffffff"/> <!-- USA -->
  <rect x="120" y="220" width="8" height="3" fill="#000033"/>
  <rect x="130" y="220" width="8" height="6" fill="#ffffff"/> <!-- Russia -->
  <rect x="130" y="220" width="8" height="3" fill="#cc0000"/>
  <rect x="140" y="220" width="8" height="6" fill="#ffffff"/> <!-- Europe -->
  <rect x="140" y="220" width="8" height="3" fill="#001133"/>
  <circle cx="144" cy="223" r="1" fill="#ffff00"/>

  <!-- Earth below -->
  <circle cx="200" cy="320" r="80" fill="#4a90e2"/>
  <circle cx="200" cy="320" r="70" fill="#5ba0f2"/>
  <circle cx="200" cy="320" r="60" fill="#6bb0ff"/>
  <path d="M 200 260 Q 220 270 240 280 Q 220 290 200 300 Q 180 290 160 280 Q 180 270 200 260 Z" fill="#228b22"/>
  <path d="M 180 270 Q 190 275 200 280 Q 210 275 220 270 Q 200 265 180 270 Z" fill="#32cd32"/>

  <!-- Name Badge -->
  <rect x="125" y="20" width="150" height="25" rx="5" fill="#ffffff" opacity="0.9"/>
  <text x="200" y="35" text-anchor="middle" fill="#000033" font-family="Arial, sans-serif" font-size="14" font-weight="bold">International Space Station</text>

  <!-- Stars -->
  <circle cx="50" cy="50" r="1" fill="#ffffff" opacity="0.8"/>
  <circle cx="350" cy="70" r="1.5" fill="#ffffff" opacity="0.6"/>
  <circle cx="100" cy="100" r="1" fill="#ffffff" opacity="0.7"/>
  <circle cx="300" cy="150" r="1.2" fill="#ffffff" opacity="0.5"/>
  <circle cx="250" cy="200" r="1" fill="#ffffff" opacity="0.6"/>
</svg>

